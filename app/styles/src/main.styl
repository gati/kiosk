@font-face {
	font-family: 'United';
	src: url('/fonts/UnitedSerifSemiExt-Bold.otf'); /* IE9 Compat Modes */
}
*
	font-family:'Helvetica-Neue-Ultralite', Helvetica
	font-weight:300
body
	overflow:hidden
	padding:0
	margin:0
	background:#A8DDE1;
ul, li
	padding:0
	margin:0
	list-style:none;
		
#background
	width:100%;
	right:0
	position:absolute;
	top:0;
	background:rgba(166,192,215,1)
	height:100%
header
	color: #fff;
	position: fixed;
	top: 0;
	width: 100%;
	z-index: 1;
	text-align: center;
	margin: 25px 0 0 10%;
	-webkit-perspective: 700;
	-webkit-transform: scale(1.2);
	nav
		width:200%;
		margin:20px auto;
		text-align center;
		-webkit-transform:rotateY(40deg)
		-webkit-transform-origin: 0 0;

		div.option
			width:150px;
			float:left;
			position:relative;
			top:4px;
			z-index:4
			-webkit-transition: all 0.2s ease-in-out;
			.top
				background-position:50% -35%;
				background-size:32%;
			.front
				background-position:50% 119%;
				background-size:32%;
			.venue
				background-image:url('/images/venue.png');
				background-repeat:no-repeat;
			.shuttle
				background-image:url('/images/shuttle.png');
				background-repeat:no-repeat;
				&.top
					background-position:50% 13%;
					background-size:42%;
				&.front
					background-position:50% 103%;
					background-size:42%;
			.social
				background-image:url('/images/social.png');
				background-repeat:no-repeat;
				&.top
					background-position:50% 12%;
					background-size:42%;
				&.front
					background-position:50% 127%;
					background-size:42%;
			.updates
				background-image:url('/images/updates.png');
				background-repeat:no-repeat;
				background-size:40%;
				&.top
					background-position:50% -10%;
				&.front
					background-position:50% 84%;
			.trending
				background-image:url('/images/trending.png');
				background-repeat:no-repeat;
				&.top
					background-position:50% -36%;
				&.front
					background-position:50% 117%;
			.map
				background-image:url('/images/map.png');
				background-repeat:no-repeat;
				background-size: 50%;
				&.top
					background-position: 48% -14%;
				&.front
					background-position:50% 109%;
			.info
				background-image:url('/images/info.png');
				background-repeat:no-repeat;
				background-size: 45%;
				&.top
					background-position: 50% -5%;
				&.front
					background-position:50% 117%;
			&.selected
				.box.logo
					-webkit-transform: rotateY(1deg) rotateX(-351deg) rotateZ(0deg);

				.title
					color:rgba(192,81,57,1)
				.box
					-webkit-transform: rotateY(23deg) rotateX(-408deg);
				.top
					background-color:rgba(246,148,96,1)
				.front
					background-color:#52270F;
				.venue
					background-image:url('/images/venue-active.png');
					background-repeat:no-repeat;
				.shuttle
					background-image:url('/images/shuttle-active.png');
					background-repeat:no-repeat;
				.social
					background-image:url('/images/social-active.png');
					background-repeat:no-repeat;
				.updates
					background-image:url('/images/updates-active.png');
					background-repeat:no-repeat;
				.trending
					background-image:url('/images/trending-active.png');
					background-repeat:no-repeat;
				.info
					background-image:url('/images/info-active.png');
					background-repeat:no-repeat;
				.map
					background-image:url('/images/map-active.png');
					background-repeat:no-repeat;
		.logo
			background: url("/images/logo.png") 0 0 no-repeat;
			-webkit-transform: rotateY(0deg) rotateX(0deg) rotateZ(0deg);
			left: -18px;
			height: 100px;
			background-size: 95%;
			width: 175px;
			top: -1px;

					
		div.options
			margin-left:150px;
		div.title
			color:rgba(80,83,89,1);
			position: absolute;
			z-index: 5;
			font-size: 1.5em;
			font-family:"United", Georgia
			text-align: center;
			width: 100%;
			-webkit-transition:all 0.5s ease-in-out;
			
#kiosk
	text-align:center;
	overflow hidden;
	z-index:10;
	-webkit-transition: -webkit-transform 2.5s ease-in-out
	.slide
		height:100%;
		max-height:100%
		width:100%
		display:block
		float:left;
		margin-top:215px;
		z-index:10;
		.content
			width:80%;
			height:80%;
			margin:0 auto;
			iframe
				width:100%;
				height:100%;
				margin:0 auto
				box-shadow:13px 13px 0px rgba(246,149,97,0.4);
				border:20px solid #fff
			.image
				width:100%;
				z-index:10;
			h2.sub
				font-style:italic
	.slide.visible
		opacity:1
	[slide-type="schedule"]
		ul
			margin-top:5px;
			li
				border-bottom:1px solid #333
				padding:10px 0
				overflow:hidden;
				.event-cell
					padding:5px 15px 5px 5px
					max-width:33%
					float:left;
					text-align:left;
				.event-name
					font-size:1.2em
					font-style:italic
					max-width:40%
					word-wrap:break-word;
			li.header
				background:#ccc
				text-align center

	[slide-type="image"]
	[slide-type="url"]
	[slide-type="updates"]
	[slide-type="venue"]
		background:transparent;
	
	#maps
		float:right;
		width:50%;
		.map 
			float:right;
			background:#fff;
			margin:5px;
			padding:10px;
			border-radius:5px;
			box-shadow:10px 12px 0px rgba(246,149,97,0.4);

			img 
				width:100%;
	
	#events-list
		width:48%;
		float:left;
		li 
			overflow:hidden
		li.header
			margin-top:20px;
			height:80px;
			text-transform:uppercase;
			.prism-container
				float:right;
				margin-right:20px;
				.text
					position: relative;
					top: -26px;
					font-size: 1.4em;
					left:8px
				.prism
					position:relative;
					left:0;
					-webkit-transform: rotateY(-30deg) rotateX(8deg);
					figure.top 
						background-color:#f69563
					figure.bottom
						background-color:#5f2300
					figure.front
						background-color:#f69563
					figure.back
						background-color:#5f2300
					figure.left
						background-color:#5f2300
					figure.right
						background-color:#5f2300

		li.header:first-child
			margin-top:0;
		li.event 
			color:rgba(105,61,40,1);
			font-size:1.3em;
			text-shadow:2px 2px 0px #bbe7f6;
			padding:5px 0;
			margin-bottom:5px;
			text-align:right;
			.event-name, .room
				padding:2px 5px;
			.room
				font-style:italic;


.container {
  width: 157px;
  height: 75px;
  position: relative;
  top:-5px;
}

.box {
  width: 100%;
  height: 100%;
  position: absolute;
  z-index:1;
  -webkit-transform-style: preserve-3d;
  -webkit-transform: rotateY(23deg) rotateX(-46deg);
  -webkit-transition:all 0.5s ease-in-out
  left:-44px
}

.box figure {
  display: block;
  position: absolute;
}

.box .front,
.box .back {
  width: 157px;
  height: 50px;
}

.box .top,
.box .bottom {
  width: 157px;
  height: 50px;
}

.box .front  { -webkit-transform: rotateY(   0deg ) translateZ(  24px ); }
.box .back   { -webkit-transform: rotateX( 180deg ) translateZ(  24px ); }
.box .top    { -webkit-transform: rotateX(  90deg ) translateZ( 26px ); }
.box .bottom { -webkit-transform: rotateX( -90deg ) translateZ( 26px ); }

nav 
	.front
		background-color:#151618;
nav
	.top
		background-color:#313A40;


//bubbles
#announcement-bubbles
	overflow:hidden

.bubble
	background:#fff;
	color:rgba(238,99,75,1)
	padding:30px 35px;
	min-width:38%;
	box-shadow:10px 12px 0px rgba(246,149,97,0.4);
	margin:10px 15px 60px 10px;
	position:relative;
	float:left;
	text-align:left;
	max-width:20%;
	&.drop
		background:#fff;
	&:before
		content: "";
		position: absolute;
		bottom: -47px;
		left: 32px;
		border-width: 0 0 35px 35px;
		border-style: solid;
		border-color: transparent rgba(246,149,97,0.4);

	&:after
		content: "";
		position: absolute;
		bottom: -40px;
		left: 20px;
		border-width: 0 0 40px 40px;
		border-style: solid;
		border-color: transparent #fff
	&.main
		width:90%;
		max-width:100%;
	.headline
		font-size:2.5em;
		font-family:"United", Georgia;
		margin:10px 0;
	.text 
		font-family:Arial,sans;
		color:rgba(238,98,75,1);
		margin:20px 0;
		font-size:1.2em;
		line-height:1.5em;
	.timestamp
		text-align:right;
		font-size:11px;
		font-style:italic;
		margin:10px 0;
.stripes
	background-color: rgba(239,107,50,1);
	background-image: -webkit-gradient(linear, 0 0, 100% 100%, color-stop(.25, rgba(255, 255, 255, .2)), color-stop(.25, transparent), color-stop(.5, transparent), color-stop(.5, rgba(255, 255, 255, .2)), color-stop(.75, rgba(255, 255, 255, .2)), color-stop(.75, transparent), to(transparent));
	background-image: -webkit-linear-gradient(-45deg, rgba(255, 255, 255, .2) 25%, transparent 25%, transparent 50%, rgba(255, 255, 255, .2) 50%, rgba(255, 255, 255, .2) 75%, transparent 75%, transparent);
	background-image: -moz-linear-gradient(-45deg, rgba(255, 255, 255, .2) 25%, transparent 25%, transparent 50%, rgba(255, 255, 255, .2) 50%, rgba(255, 255, 255, .2) 75%, transparent 75%, transparent);
	background-image: -ms-linear-gradient(-45deg, rgba(255, 255, 255, .2) 25%, transparent 25%, transparent 50%, rgba(255, 255, 255, .2) 50%, rgba(255, 255, 255, .2) 75%, transparent 75%, transparent);
	background-image: -o-linear-gradient(-45deg, rgba(255, 255, 255, .2) 25%, transparent 25%, transparent 50%, rgba(255, 255, 255, .2) 50%, rgba(255, 255, 255, .2) 75%, transparent 75%, transparent);
	background-image: linear-gradient(-45deg, rgba(255, 255, 255, .2) 25%, transparent 25%, transparent 50%, rgba(255, 255, 255, .2) 50%, rgba(255, 255, 255, .2) 75%, transparent 75%, transparent);
	-webkit-background-size: 20px 20px;

.vertical-stripes
	background-color: rgba(239,107,50,1);
	background-image: -webkit-gradient(linear, 0 0, 100% 0, color-stop(.5, rgba(255, 255, 255, .2)), color-stop(.5, transparent), to(transparent));
	background-image: -webkit-linear-gradient(0deg, rgba(255, 255, 255, .2) 50%, transparent 50%, transparent);
	background-image: -moz-linear-gradient(0deg, rgba(255, 255, 255, .2) 50%, transparent 50%, transparent);
	background-image: -ms-linear-gradient(0deg, rgba(255, 255, 255, .2) 50%, transparent 50%, transparent);
	background-image: -o-linear-gradient(0deg, rgba(255, 255, 255, .2) 50%, transparent 50%, transparent);
	background-image: linear-gradient(0deg, rgba(255, 255, 255, .2) 50%, transparent 50%, transparent);
	-webkit-background-size: 20px 20px;

.arrow
