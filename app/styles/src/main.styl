#scene
	width:100%;
	
	display:none;
	section.skyline
		background:url('/images/skyline.png') 0 80% no-repeat;
		background-repeat:repeat-x;
		height:100%;
		width:100%;
		position:absolute;
		bottom:0;
		z-index:-1;
		-webkit-transition:-webkit-transform 2s linear;

	section.crowd
		background:url('/images/crowd.png') 0 90% no-repeat;
		background-repeat:repeat-x;
		// background-size:100%;
		height:100%;
		width:100%;
		position:absolute;
		bottom:0;
		z-index:-1;
		-webkit-transition:-webkit-transform 2s linear;
	section.characters
		z-index:100000;
		position:absolute;
		bottom:0;
		-webkit-transition:-webkit-transform 2s linear;
		.group
			width:100%;
			float:left;
			display:block
			position:relative;
			img
				display:block

#sponsor
	position:fixed;
	bottom:0;
	z-index:10000000;
	width:100%;
	text-align:center;
	padding:5px;
	img
		margin:0 auto;



.prism-container
	width: 150px;
	height: 50px;
	position: relative;
	
	.prism
		width: 100%;
		height: 100%;
		position: absolute;
		-webkit-transform-style: preserve-3d;
		figure
			display: block;
			position: absolute;
			border: 2px solid transparent;

		.front
			width: 150px;
			height: 48px;

		.right
			width: 120px;
			height: 48px;
			left: 74px;
			top: 1px;
		.bottom 
			width: 150px;
			height: 30px;
			top: 36px;
			left: -20px;

		.front  { -webkit-transform: rotateY(   0deg ) translateZ(  50px ); }
		.right  { -webkit-transform: rotateY(113deg) translateZ(-2px);}
		.bottom { -webkit-transform: rotateX(-70deg) translateZ(0px);}

.shadow 
	box-shadow:13px 13px 0px rgba(246,149,97,0.4);
.left 
	float:left;
.right 
	float:right;
